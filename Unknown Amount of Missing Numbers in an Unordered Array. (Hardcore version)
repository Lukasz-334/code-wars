#include <algorithm>
#include <vector>

std::vector<unsigned int> missNumsFinder(std::vector<unsigned int>& arr) {
    auto it = std::max_element(arr.begin(), arr.end());
    std::vector<bool> bits(*it + 1, false);
    std::vector<unsigned int> solv;

    for (int x : arr) {
        bits[x] = true;
    }

    for (int i = 1; i <= *it; ++i) {
        if (!bits[i]) {
            solv.push_back(i);
        }
    }

    return solv;
