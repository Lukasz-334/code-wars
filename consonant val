#include <iostream>
#include <string>

int solve(const std::string& s) {
    int sum = 0;
    int solve = 0;

    for (auto el : s) {
        if ((el != 'a') && (el != 'e') && (el != 'i') && (el != 'o') && (el != 'u')) {
            sum += el - 96;
        } else {
            if (sum > solve) {
                solve = sum;
            }
            sum = 0;
        }
    }
    if(sum>solve){
      solve = sum;
    } 
    return solve;
}
